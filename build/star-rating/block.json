{
  "apiVersion": 2,
  "name": "b-blocks/star-rating",
  "title": "Star Rating",
  "description": "Gutenberg editing with easy-to-use, responsive star-rating more.",
  "category": "bBlocks",
  "keywords": [
    "star-rating",
    "Rating",
    "Gutenberg"
  ],
  "textdomain": "star-rating",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "ratingScale": {
      "type": "number",
      "default": 5
    },
    "rating": {
      "type": "number",
      "default": 4.3
    },
    "iconStyle": {
      "type": "string",
      "default": "solid"
    },
    "prefix": {
      "type": "string",
      "default": ""
    },
    "gap": {
      "type": "string",
      "default": "10px"
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "textTypo": {
      "type": "object",
      "default": {
        "fontSize": "16px"
      }
    },
    "textColor": {
      "type": "string",
      "default": "#7a7a7a"
    },
    "textShadow": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "style": "file:./view.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}