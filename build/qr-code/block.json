{
  "apiVersion": 2,
  "name": "b-blocks/qr-code",
  "title": "QR Code",
  "description": "Show QR Code in posts and pages.",
  "category": "bBlocks",
  "keywords": [
    "QR scanner",
    "QR code",
    "Code"
  ],
  "textdomain": "b-blocks",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "qrAlign": {
      "type": "string",
      "default": "center"
    },
    "url": {
      "type": "string",
      "default": ""
    },
    "dimension": {
      "type": "number",
      "default": 180
    },
    "color": {
      "type": "string",
      "default": "#000"
    },
    "bgColor": {
      "type": "string",
      "default": "#fff"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "style": "file:./view.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}